{
  "name": "velvet-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "postcss": "sass ./src/assets/scss/styles.scss ./src/assets/css/styles.css && postcss ./src/assets/css/styles.css -o ./src/assets/css/styles.css",
    "sass": "sass ./src/assets/scss:./src/assets/css/",
    "sass-min": "sass ./src/assets/scss:./src/assets/css/ --style compressed"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.2",
    "@simonwep/pickr": "^1.9.0",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "apexcharts": "^3.45.2",
    "axios": "^1.7.3",
    "crypto-js": "^4.2.0",
    "firebase": "^10.8.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "nanoid": "^5.0.5",
    "react": "^18",
    "react-apexcharts": "^1.4.1",
    "react-bootstrap": "^2.10.0",
    "react-datepicker": "^6.6.0",
    "react-dom": "^18",
    "react-helmet-async": "^2.0.4",
    "react-persist": "^1.0.2",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.1",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "sass": "^1.70.0",
    "simplebar-react": "^3.2.4",
    "vite": "^5.3.5"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "eslint": "^8",
    "eslint-config-next": "14.1.0"
  }
}
